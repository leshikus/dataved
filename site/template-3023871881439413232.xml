<script type='text/javascript'>
  function getCookie(key) {
    var pairs = document.cookie.split(&#39;; &#39;);
    for (var i = 0, pair; pair = pairs[i] &amp;&amp; pairs[i].split(&#39;=&#39;); i++) {
      if (decodeURIComponent(pair[0]) === key) return decodeURIComponent(pair[1] || &#39;&#39;); // IE saves cookies with empty string as &quot;c; &quot;, e.g. without &quot;=&quot; as opposed to EOMB, thus pair[1] may be undefined
      }
      return null;
    }

  function setCookie(key, value) {
    var d = new Date();
    d.setDate(d.getDate() + 365);

    var c = encodeURIComponent(key) + &#39;=&#39; + encodeURIComponent(value)
      + &#39;; path=/; expires=&#39; + d.toUTCString() 
    return (document.cookie = c);
  }
</script>

<b:if cond='data:blog.url == data:blog.homepageUrl'>
